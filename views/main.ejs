  <body class=" bg-dark">

<br>

<main class="form-signin">
  <form action="/articles" method="post">
    <div class="form-floating">
      <input type="text" class="form-control" id="floatingInput" name="title" placeholder="name">
      <label for="floatingInput">title</label>
    </div>
    <div class="form-floating">
      <input type="text" class="form-control middle" id="floatingPassword" name="content" placeholder="last name">
      <label for="floatingPassword">content</label>
    </div>
    <button class="w-100 btn btn-lg btn-primary" type="submit">Add</button>
  </form>
</main>
<script type='text/javascript' src='config.js'></script>
<script type='text/javascript' src='script.js></script>


  </body>
</html>